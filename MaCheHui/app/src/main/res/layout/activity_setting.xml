<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ebebeb"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/wash_head_ll"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/pub_ui_head_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/wash_cur_deatail_tv_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="设置 "
            android:textColor="@color/white"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/setting_back_button"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:background="@drawable/wash_back_bg_selector" />
    </RelativeLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#5b5b5b"
        android:text="" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ebebeb"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/feedback_rl_setting"
            style="@style/my_infor_rl"
            android:visibility="gone" >

            <TextView
                android:id="@+id/textView6"
                style="@style/my_infor_tv"
                android:text="意见反馈" />

            <ImageView
                android:id="@+id/imageView6"
                style="@style/my_infor_right_iv" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/update_rl_setting"
            style="@style/my_infor_rl"
            android:layout_marginTop="1px" >

            <TextView
                android:id="@+id/textView8"
                style="@style/my_infor_tv"
                android:text="版本更新" />

            <ImageView
                android:id="@+id/imageView8"
                style="@style/my_infor_right_iv" />

            <TextView
                android:id="@+id/update_tv_setting"
                style="@style/my_infor_center_tv"
                android:layout_toLeftOf="@+id/imageView8"
                android:text="已是最近版本" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/about_rl_setting"
            style="@style/my_infor_rl"
            android:layout_marginTop="1px" >

            <TextView
                android:id="@+id/textView9"
                style="@style/my_infor_tv"
                android:text="关于我们" />

            <ImageView
                android:id="@+id/imageView9"
                style="@style/my_infor_right_iv" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/share_rl_setting"
            style="@style/my_infor_rl" >

            <TextView
                android:id="@+id/textView10"
                style="@style/my_infor_tv"
                android:text="分享码车惠给好友" />

            <ImageView
                android:id="@+id/imageView10"
                style="@style/my_infor_right_iv" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom"
            android:orientation="vertical" >

            <Button
                android:id="@+id/goout"
                android:layout_width="match_parent"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:layout_height="50dp"
                android:background="@drawable/ib_gray_button"
                android:layout_marginBottom="50dp"
                android:text="退出登录" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>